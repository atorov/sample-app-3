  "main": "src/index.jsx",
  "scripts": {
    "clean": "rm -rf ./build",
    "build": "npm run build:prod",
    "build:prod": "npm run clean && NODE_ENV=production webpack",
    "build:prod:stats": "npm run clean && NODE_ENV=production webpack --json > stats.json --profile --progress --display-reasons",
    "serve": "npm run build:prod && serve ./build/www",
    "start": "nodemon --watch webpack.*.js --exec \"NODE_ENV=development PORT=9080 webpack-dev-server\""
  },
